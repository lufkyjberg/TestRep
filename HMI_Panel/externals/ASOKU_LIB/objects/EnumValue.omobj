	<do-on access-modifier="private" name="Handler_3" display-name="Handler_3" ver="5" event="MouseClick">
		<body kind="om">
			<![CDATA[if(ApParName.Advised == true)
{
  if (AsokuMain.WindowGroup_InfoViewMessages.WindowCount < 1) Open_AlarmView.Invoke();
  else 
  {
    AsokuMain.WindowGroup_InfoViewMessages.SendToAll("sendmessage", ParDevPath.Value);
  }
}
]]>
		</body>
	</do-on>
	<do-on access-modifier="private" name="Handler_4" display-name="Handler_4" ver="5" event="MouseRightClick">
