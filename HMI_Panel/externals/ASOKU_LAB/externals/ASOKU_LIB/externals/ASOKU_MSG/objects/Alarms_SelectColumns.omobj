	<object access-modifier="private" name="CurrentState" display-name="CurrentState" uuid="577d9d2f-132a-423f-a5bc-6f3d78af7dbe" base-type="notifying_string" base-type-id="14976fbf-36ab-415f-abc3-9f8fdc217351" ver="5">
		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="ValueChanged">
			<body kind="javascript">
				<![CDATA[var columns = [];
if (value != "")
	columns = JSON.parse(value);

var count = 0;
for(let i = 0; i < Checkboxes.Length; i++)
{
	if (i < columns.length)
	{
		Checkboxes[i].Text = columns[i].Title;
		Checkboxes[i].State = columns[i].State;		
		Checkboxes[i].Visible = true;
		count += 1;
	}
	else
	{
		Checkboxes[i].Visible = false;
	}
}

// подстраиваем высоту окна
WindowHeight = 10 + 25 * count + 10 + OkButton.Height + 10;]]>
			</body>
		</do-on>
	</object>
