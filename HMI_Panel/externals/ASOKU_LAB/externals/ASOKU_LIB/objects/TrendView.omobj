}

else if (messageType == "sendmessageSimple")
{
    Ap_Source_Par.Path = messageData;     //перезаписываем новый путь до тега
    Trends.ForcedAddItem(Ap_Par_Value.Tag,Ap_Par_Value.Units, Ap_Par_Value.Description);// добавляем новую шкалу
    Trends.SetIndividualRange(Ap_Par_Value.Tag, 0, 100); // выставляем диапазоны отображения параметра
}
 
]]>
