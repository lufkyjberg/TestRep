		<do access-modifier="private" name="OrdinaryMessageCorrection" display-name="OrdinaryMessageCorrection" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="javascript">
				<![CDATA[// Функция корректировки количества обычных сообщений
// Если строк больше допустимого, то удаляются самые старые (являются первыми в модели данных) НЕ ОСОБОВАЖНОЕ события 

var count = TableModel_1.GetRowCount();
if (count > MaxVisibleOrdinaryEvents)
{
	//log("Rows in table: " + count + " (max = " + MaxVisibleOrdinaryEvents + ")");
	//var start = Date.now();
	
	// Получаем массивы значений важности из запросов
	var severityArray1 = JSON.parse("[" + ApAlarmQueue_1.severityArray + "]"); // особо важные
	var severityArray2 = JSON.parse("[" + ApAlarmQueue_2.severityArray + "]");
	var severityArray3 = JSON.parse("[" + ApAlarmQueue_3.severityArray + "]");

	// определяем индексы столбцов, чтобы не вычислять их в цикле
	var severityIndex = Table_Operative_mode.MapColumnToModel(Table_Operative_mode.Colum_6.GetIndex());
	var ackedIndex = Table_Operative_mode.MapColumnToModel(Table_Operative_mode.Colum_7.GetIndex());
	var sourceIndex = Table_Operative_mode.MapColumnToModel(Table_Operative_mode.Colum_source_not_show.GetIndex());
	var conditionIndex = Table_Operative_mode.MapColumnToModel(Table_Operative_mode.Colum_11_not_show.GetIndex());
	var activeTimeIndex	= Table_Operative_mode.MapColumnToModel(Table_Operative_mode.Colum_2.GetIndex());	

	var row = 0; // индекс проверяемой строки
	var fromRow = 0; // индекс строки, начиная с которой требуется удаление
	
	// Пока количество обработанных событий не сравняется с максимальным
	while (count > MaxVisibleOrdinaryEvents)
	{
		var severity = TableModel_1.GetCellData(row, severityIndex);
		var acked = TableModel_1.GetCellData(row, ackedIndex);
		
		// проверяем, относится ли событие к особо важным
		var isVeryImportant = severityArray1.includes(severity);

		// если обычное сообщение или квитированное, то удаляем его
		if (!isVeryImportant || acked)
		{
			// выбираем очередь звуков в зависимости от важности события
			var alarmQueue;
			if (isVeryImportant)
				alarmQueue = ApAlarmQueue_1;
			else if (severityArray2.includes(severity))
				alarmQueue = ApAlarmQueue_2;
			else if (severityArray3.includes(severity))
				alarmQueue = ApAlarmQueue_3;
			else alarmQueue = ApAlarmQueue_4;

			// удаляем запись из очереди активных алармов			
			if (alarmQueue.Length > 0)
			{
				// получаем необходимую информацию об удаляемом событии
				var source = TableModel_1.GetCellData(row, sourceIndex);
				var condition = TableModel_1.GetCellData(row, conditionIndex);
				var activeTime = TableModel_1.GetCellData(row, activeTimeIndex);
				//log("find " + activeTime);

				alarmQueue.MoveFirst();
				while(true)
				{
					// разбираем информацию о событии из очереди активных алармов
					var alarmInfoStr = alarmQueue.CurrentAlarm();
					var alarmInfo = JSON.parse(alarmInfoStr);
					
					// если время активации в очереди больше времени активации удаляемого события, то можно прекратить поиск активных алармов в очереди
					if (alarmInfo.activeTime > activeTime)
						break;					
					
					// проверяем совпадение по трем признакам: источнику, условию и времени активации (пока другого способа пока нет #23750)
					// примечание: время активации удаляемого события и время из очереди преобразуются в number с потерей точности (#23748)
					if (alarmInfo.source == source && alarmInfo.condition == condition && alarmInfo.activeTime == activeTime)
					{
						alarmQueue.RemoveCurrent();
						//log("removed " + alarmInfoStr);
					}

					if (!alarmQueue.MoveNext())
						break;
				}
			}

			// событие нужно удалить, но мы просто переходим к следующей строке,
			// а удалять будем сразу несколько строк, когда дойдем до неудаляемой строки
			row += 1;
		}
		else
		{
			// это событие нужно оставить, а предыдущие удалить
			if (row > fromRow)
			{
				// удаляем строки из таблицы
				TableModel_1.RemoveRows(fromRow, row - fromRow);

				// после удаления строк индекс необходимо сместить на начало удаленного диапазона
				row = fromRow;
			}

			// переходим к следующей строке и смещаем начало удаляемого диапазона
			row += 1;
			fromRow = row;
		}
		
		count--;
	}

	// последние неудаленные события
	if (row > fromRow)
		TableModel_1.RemoveRows(fromRow, row - fromRow);

	//log("OrdinaryMessageCorrection: " + (Date.now() - start) + " (rows after correction: " + TableModel_1.GetRowCount() + ")");
}
]]>
			</body>
		</do>
		<do access-modifier="private" name="RemoveDuplicates" display-name="RemoveDuplicates" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="javascript">
				<![CDATA[// Удаление дублирующихся сообщений в оперативной таблице
Main_panel.private.DuplicateCheck = false;

// Формируем массив новых событий
var insertedRows = [];
for (let i = first; i <= last; i++)
	insertedRows.push(TableModel_1.GetRowDataAsJson(i));

if (insertedRows.length > 0)
{
	//var start = Date.now();

	var row = 0; // индекс проверяемой строки
	var fromRow = 0; // индекс строки, начиная с которой требуется удаление
	
	// Проходим по таблице, удаляя совпадающие строки
	var firstInsertedRow = first;
	while (row < firstInsertedRow)
	{
		var rowJson = TableModel_1.GetRowDataAsJson(row);

		var indexMatch = insertedRows.indexOf(rowJson);
		if (indexMatch == -1)
		{
			// это событие нужно оставить, а предыдущие удалить
			if (row > fromRow)
			{
				// удаляем строки из таблицы
				var removed = row - fromRow;
				TableModel_1.RemoveRows(fromRow, removed);
				//log("Rows removed from " + fromRow + " to " + row);
				
				// смещаем начальный индекс строки для новых событий
				firstInsertedRow -= removed;

				// после удаления строк индекс необходимо сместить на начало удаленного диапазона
				row = fromRow;
			}

			// переходим к следующей строке и смещаем начало удаляемого диапазона
			row += 1;
			fromRow = row;
		}
		else
		{
			// событие нужно удалить, но мы просто переходим к следующей строке,
			// а удалять будем сразу несколько строк, когда дойдем до неудаляемой строки
			row += 1;
			
			// удаляем из массива новых строк, чтобы дальнейшие проверки выполнялись быстрее
			insertedRows.splice(indexMatch, 1);
		}
	}
	
	// последние неудаленные события
	if (row > fromRow)
	{
		TableModel_1.RemoveRows(fromRow, row - fromRow);
		//log("Finish rows removed from " + fromRow + " to " + row);
	}
	
	//log("RemoveDuplicates: " + (Date.now() - start) + " (rows after removing: " + TableModel_1.GetRowCount() + ")");
}]]>
			</body>
