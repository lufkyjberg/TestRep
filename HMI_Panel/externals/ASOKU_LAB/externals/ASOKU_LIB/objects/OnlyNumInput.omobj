	<do-on access-modifier="private" name="DeleteNotNumsFromInput" display-name="DeleteNotNumsFromInput" ver="5" event="TextChanged">
		<body kind="javascript">
			<![CDATA[const charsArray = this.Text.split("");

for (let i = 0; i < charsArray.length; i++) {
	if (charsArray[i] !== "." && isNaN(charsArray[i])) {
		charsArray.splice(i, 1);
		i--;
	}
}

const newString = charsArray.join("");

this.Text = newString;]]>
		</body>
	</do-on>
