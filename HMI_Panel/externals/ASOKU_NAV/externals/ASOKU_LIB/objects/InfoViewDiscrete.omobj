	<object access-modifier="private" name="YesAction_UD_AppSettings" display-name="действия при нажатии на Да в диалоговом окне" uuid="6afebd81-95c9-40e0-b076-8367515c8bfb" base-type="Action" base-type-id="a9ee9770-1c4a-44c9-b815-157d9fc2ab95" ver="5" description="" cardinal="1">
		<designed target="Enabled" value="true" ver="5"/>
		<designed target="InvokeTrigger" value="false" ver="5"/>
		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="Invoked">
			<body kind="om">
				<![CDATA[// Применение измененных значений
                                                  
 if(Text_Name.FontColor == AsokuColor.asokuSky)    Ap_Par_Name.Value = Text_Name.Text;        // наименование
  
 if(Text_Comment.FontColor == AsokuColor.asokuSky) Ap_Par_Comment.Value = Text_Comment.Text;  // комментарий пользователя
 
  
 if(Text_Min.FontColor == AsokuColor.asokuSky) // минимальное и максимальное значение
 {
    Ap_Par_MinValue.Value = String.ToFloat(Text_Min.Text,0); 
    if (restoreMin == false)Button_ResetMin.Visible = Text_Min.ReadOnly;
 }
 if(Text_Max.FontColor == AsokuColor.asokuSky)
 {
    Ap_Par_MaxValue.Value = String.ToFloat(Text_Max.Text,0); 
    if (restoreMax == false)Button_ResetMax.Visible = Text_Min.ReadOnly;
 } 
 
 if(Text_DisplayOn.FontColor == AsokuColor.asokuSky) Ap_Par_DisplayOn.Value =  Text_DisplayOn.Text; // формат 
 if(Text_DisplayOff.FontColor == AsokuColor.asokuSky) Ap_Par_DisplayOff.Value =  Text_DisplayOff.Text; // формат  
  
 if((Text_Name.FontColor == AsokuColor.asokuSky) || (Text_Comment.FontColor == AsokuColor.asokuSky) || (Text_Min.FontColor == AsokuColor.asokuSky) || (Text_Max.FontColor == AsokuColor.asokuSky) || (Text_DisplayOn.FontColor == AsokuColor.asokuSky) ||(Text_DisplayOff.FontColor == AsokuColor.asokuSky)) 
{
  TakeMessage(); // выдача сообщения об изменениях пользователя //
  SetColorField();                            // установка первоначального цвета шрифта у полей
  Button_Acc.FontColor = AsokuColor.asokuWhite;  // сброс цвета символов у кнопки Применить
}    
      
       
      
      ]]>
			</body>
		</do-on>
	</object>
