					<do access-modifier="private" name="UsedFiltersCombine" display-name="UsedFiltersCombine" ver="5" type="string" type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d">
						<body kind="javascript">
							<![CDATA[//read
var used_filters_array = [];
if (used_filters != "" && used_filters.length>0)
	used_filters_array = JSON.parse(used_filters);
var allkeys = JSON.parse(private.ViewFilter.GetColumns());
//collect
var data = {}; var keys = [];
function DataCollect(key,str)
{
	if (str != "")
		{
			if (keys.includes(key)==false) keys.push(key);
			if (data[key]==undefined) data[key] = [];
			if (data[key].includes(str)==false)
			{
				data[key].push(str);
			}
		}
}
for (let i = 0; i < Main_panel.private.PresetFilters.name.Length; i++)
if (used_filters_array.includes(Main_panel.private.PresetFilters.name[i]))
{
	var _obj = {'time':Main_panel.private.PresetFilters.time[i], 'active_time':Main_panel.private.PresetFilters.active_time[i], 'deactive_time':Main_panel.private.PresetFilters.deactive_time[i], 'severity':Main_panel.private.PresetFilters.severity[i], 'group':Main_panel.private.PresetFilters.group[i], 'message':Main_panel.private.PresetFilters.message[i], 'ack':Main_panel.private.PresetFilters.ack[i], 'ack_time':Main_panel.private.PresetFilters.ack_time[i], 'actor_id':Main_panel.private.PresetFilters.actor_id[i], 'ack_comment':Main_panel.private.PresetFilters.ack_comment[i]};
	_obj[Main_tables_Operative.Table_Operative_mode.Colum_5.Id] = Main_panel.private.PresetFilters.source[i];
	var _str = JSON.stringify(_obj);
	var sj = _str!="" ? JSON.parse(_str) : {};
	for (let j = 0; j < allkeys.length; j++)
		DataCollect(allkeys[j], sj[allkeys[j]]);
}
//modify
var ackdata = data[Main_tables_Operative.Table_Operative_mode.Colum_7.Id];
if (ackdata != undefined)
	{
		//только в случае наличия единственного условия требуется фильтрация по ack
		var conditions = ackdata.map(function(_value){return Main_panel.private.PresetFilters.GetAckCondition(_value);});
		if (conditions.includes("") || conditions.length!=1)
			keys = keys.filter(function(_value){return _value!=Main_tables_Operative.Table_Operative_mode.Colum_7.Id;});
	}
//result
var result = {};
for (let i = 0; i < keys.length; i++)
	result[keys[i]] = data[keys[i]].length==1 ? data[keys[i]][0] : data[keys[i]].join(" ИЛИ ");
return JSON.stringify(result);]]>
						</body>
