	<do-on access-modifier="private" name="Handler_2" display-name="Handler_2" ver="5" event="KeyPress">
		<body kind="om">
			<![CDATA[if (event.KeyCode == 82) // Enter
{
	if (Buttons == 0 || Buttons == 1)
	{
		OkAction.Invoke();
		Result = 1; // Ok
	}
	else if (Buttons == 2 || Buttons == 3)
	{
		YesAction.Invoke();
		Result = 2; // Yes
	}
}
else if (event.KeyCode == 75) // Escape
{
	CancelAction.Invoke();
	Result = 4; // Cancel
}]]>
		</body>
	</do-on>
