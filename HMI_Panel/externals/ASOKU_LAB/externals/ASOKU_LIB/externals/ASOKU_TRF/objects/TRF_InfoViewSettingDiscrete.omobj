		<body kind="om">
			<![CDATA[//выдача сообщений об изменениях пользователя

if(TextEdit_Value.FontColor ==AsokuColor.asokuSky) AsokuMain.CreateUserMessage("значение имитации у сигнала " +  Ap_Source_Par.Path);

if(TextEdit_SimState.FontColor ==AsokuColor.asokuSky) AsokuMain.CreateUserMessage("качество у сигнала " +  Ap_Source_Par.Path);

if(TextEdit_DelayLimits.FontColor ==AsokuColor.asokuSky) AsokuMain.CreateUserMessage("задержки у сигнала " +  Ap_Source_Par.Path);

if(TextEdit_SuppressionTimeLimit.FontColor ==AsokuColor.asokuSky) AsokuMain.CreateUserMessage("лимита времени подавления у сигнала " +  Ap_Source_Par.Path);


if(Checkbox_SimOn.FontColor ==AsokuColor.asokuSky) AsokuMain.CreateUserMessage("флага имитации у сигнала " +  Ap_Source_Par.Path);

if(Checkbox_SP_Alarm.FontColor ==AsokuColor.asokuSky) AsokuMain.CreateUserMessage("флага аварийной сигнализации у сигнала " +  Ap_Source_Par.Path);

if(Checkbox_SP_Warning.FontColor ==AsokuColor.asokuSky) AsokuMain.CreateUserMessage("флага предупредительной сигнализации у сигнала " +  Ap_Source_Par.Path);

if(Checkbox_SP_Inverted.FontColor ==AsokuColor.asokuSky) AsokuMain.CreateUserMessage("флага инвертированного значения у сигнала " +  Ap_Source_Par.Path);

]]>
