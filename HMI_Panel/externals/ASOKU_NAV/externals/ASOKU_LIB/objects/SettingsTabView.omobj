			<![CDATA[//Определение положения окна
posWindowNum = 0;
while(posWindowNum<= AsokuMain.InfoTabViewPosNumber.Length && AsokuMain.InfoTabViewPosNumber[posWindowNum] == true )
{
  posWindowNum += 1;
}
AsokuMain.InfoTabViewPosNumber[posWindowNum] = true;

if(posWindowNum == 0)
{
  this.WindowX = 10;
  this.WindowY = 100;
}
if(posWindowNum == 1)
{
  this.WindowX = 500;
  this.WindowY = 100;
}
if(posWindowNum == 2)
{
  this.WindowX = 990;
  this.WindowY = 100;
}
if(posWindowNum == 3)
{
