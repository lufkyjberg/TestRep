	<object access-modifier="private" name="_TransferDevPath" display-name="_полный путь до трансферного узла" uuid="9a5044fa-b124-4b6c-a7f6-49300338107a" base-type="string" base-type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d" ver="5" description="" cardinal="1"/>
	<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="Opened">
		<body kind="om">
			<![CDATA[
TRF_Base.parDevPath = here.ParDevPath.Value; //задание пути параметра

//задание положения окна
posWindowNum = 0; 
while(posWindowNum<= AsokuMain.InfoTabViewPosNumber.Length && AsokuMain.InfoTabViewPosNumber[posWindowNum] == true )
{
  posWindowNum += 1;
}
if(posWindowNum == 1)
{
  this.WindowX = 400;
  this.WindowY = 10;
}
if(posWindowNum == 2)
{
  this.WindowX = 800;
  this.WindowY = 10;
}
if(posWindowNum == 3)
{
  this.WindowX = 1200;
  this.WindowY = 10;
}
if(posWindowNum == 4)
{
  this.WindowX = 400;
  this.WindowY = 500;
}
if(posWindowNum == 5)
{
  this.WindowX = 800;
  this.WindowY = 500;
}
if(posWindowNum == 6)
{
  this.WindowX = 1200;
  this.WindowY = 500;
}

Button_Acc.Enabled = false;
Button_Refresh.Enabled = false;

//заполнение списка режимов
Rectangle_1.ComboBox_ExternalMode.AddItem("Обычный");
Rectangle_1.ComboBox_ExternalMode.AddItem("Внешнее задание");
Rectangle_1.ComboBox_ExternalMode.AddItem("Симуляция");


Timer_Init.Active = true;     //запуск таймера инициализации

Button_Acc.Enabled = true;
Button_Refresh.Enabled = true;

]]>
		</body>
	</do-on>
