	</do-on>
	<do-on access-modifier="private" name="Handler_5" display-name="Handler_5" ver="5" event="MouseEnter">
		<body kind="om">
			<![CDATA[if(ApParValue.Advised == true) {
	if(ApParStatusBytes.Value != AsokuConst.StatusBytes.Changed) PenColor = AsokuColor.asokuGrey;
	PenWidth = 5;
}]]>
		</body>
	</do-on>
	<do-on access-modifier="private" name="Handler_6" display-name="Handler_6" ver="5" event="MouseLeave">
		<body kind="om">
			<![CDATA[if(ApParStatusBytes.Value == AsokuConst.StatusBytes.Changed) PenColor = AsokuColor.asokuCyan;
else if (ApParStatusBytes.Value == AsokuConst.StatusBytes.Invalid) PenColor = AsokuColor.asokuBlack;
else PenColor =  AsokuColor.asokuTransparent;
PenWidth = 3;]]>
		</body>
	</do-on>
