	<do access-modifier="private" name="RefreshSettings" display-name="Функция сброса уставок" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="javascript">
			<![CDATA[//Функция считывания уставок
for (let i = 0; i < 6; i ++){
	Rectangle_Catalogs.Checkbox_Sources[i].State = (Ap_Par_PrioritySources[i].Value != 99);
	Rectangle_Catalogs.ComboBox_Priority_Sources[i].SelectedIndex = Rectangle_Catalogs.ComboBox_Priority_Sources[i].IndexOf(Ap_Par_PrioritySources[i].ValueAsString)
}

Checkbox_ModeAvarageValueWithoutQiality.State = (Ap_Par_SelectModeBytes == 2); // Считываем значение чекбокса, котоырй на сервере релизован как отдельный режим под порядковым номером 2


//устанавливаем индекс в выпадающем списке соответсвующий порядковому номеру на сервере
if (Ap_Par_SelectModeBytes == 1) ComboBox_SelectMode.SelectedIndex = ComboBox_SelectMode.IndexOf("Среднее значение");
else if (Ap_Par_SelectModeBytes == 2) ComboBox_SelectMode.SelectedIndex = ComboBox_SelectMode.IndexOf("Среднее значение");
else if (Ap_Par_SelectModeBytes == 4) ComboBox_SelectMode.SelectedIndex = ComboBox_SelectMode.IndexOf("Выбор пользователя");
