		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="OnTimer">
			<body kind="om">
				<![CDATA[//таймер инициализации панели

here.WindowCaption = "Настройка сигнала: " + TRF_Base.Ap_Par_Description;// задать имя панели

if( isNeedUpdateSettings() == true) //сохраненные значения трансфера отличаются от сигнала
{
   Rectangle_7.Image_NeedUpdate.Visible = true;
 
  /* if(TRF_Base.isTransferActive())  //не используется
   {
    	Open_UD_impossible.message = "Настройка данного параметра сейчас активна";
    	Open_UD_impossible.header = "Невозможно открыть трансферный блок для сигнала";
    	Open_UD_impossible.Invoke();
    	return;
   }*/
   
  _TransferDevPath = TRF_Base.getTransferDpByConnection(); // получение пути до трансфера
  if (_TransferDevPath == "")
  {
     Open_UD_impossible.message = "В данный момент нет свободных трансферных блоков";
     Open_UD_impossible.header = "Невозможно открыть трансферный блок для сигнала";
     Open_UD_impossible.Invoke();
     Timer_Init.Active = false;
  }

  TRF_Base.constructor(_TransferDevPath); 
  Rectangle_7.TextEdit_TransferPath =  _TransferDevPath;
  
}
else
{
  TRF_Base.constructor(""); 
  Rectangle_7.Image_NeedUpdate.Visible = false; 
}


TRF_Base.readData(); //чтение данных
Timer_ReadElements.Active = true; // запуск таймера чтения элементов
_TransferDevPath = "";

Button_Acc.Enabled = true;
Button_Refresh.Enabled = true;

Timer_Init.Active = false;
]]>
