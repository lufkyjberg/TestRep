		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="Invoked">
			<body kind="om">
				<![CDATA[if (ApParSimOn.Value != SimulationFrame.SimulationStatusCheckbox.State) {
	ApParSimOn.Value = SimulationFrame.SimulationStatusCheckbox.State;
	SimulationFrame.SimulationStatusCheckbox.isChanged = false;
	AsokuMain.CreateUserMessage("флага имитации у сигнала " + ParDevPath);
}
if (ApParSimValue.ValueAsString != SimulationFrame.Container_SimValue.Checkbox_SimValue.Text) {
	ApParSimValue.Value = SimulationFrame.Container_SimValue.Checkbox_SimValue.State;
	SimulationFrame.Container_SimValue.Checkbox_SimValue.isChanged = false;
	AsokuMain.CreateUserMessage("значение имитации у сигнала " + ParDevPath);
}
if (ApParSimStatus.ValueAsString != SimulationFrame.Container_SimStatus.Input.Text) {
	ApParSimStatus.Value = String.ToUint2(SimulationFrame.Container_SimStatus.Input.Text, ApParSimStatus.Value);
	SimulationFrame.Container_SimStatus.Input.isChanged = false;
	AsokuMain.CreateUserMessage("качество у сигнала " + ParDevPath);
}
if (ApParDelayLimits.ValueAsString != SetpointsFrame.Container_Delay.Input.Text) {
	ApParDelayLimits.Value = String.ToUint4(SetpointsFrame.Container_Delay.Input.Text, ApParDelayLimits.Value);
	SetpointsFrame.Container_Delay.Input.isChanged = false;
	AsokuMain.CreateUserMessage("задержки у сигнала " + ParDevPath);
}
if (ApParSuppressionTimeLimit.ValueAsString != SetpointsFrame.Container_Supress.Input.Text) {
	ApParSuppressionTimeLimit.Value = String.ToUint4(SetpointsFrame.Container_Supress.Input.Text, ApParSuppressionTimeLimit.Value);
	SetpointsFrame.Container_Supress.Input.isChanged = false;
	AsokuMain.CreateUserMessage("задержки у сигнала " + ParDevPath);
}
if (ApParSTAlarm.Value != SetpointsFrame.Checkbox_SP_Alarm.State) {
	ApParSTAlarm.Value = SetpointsFrame.Checkbox_SP_Alarm.State;
	SetpointsFrame.Checkbox_SP_Alarm.isChanged = false;
	AsokuMain.CreateUserMessage("задержки у сигнала " + ParDevPath);
}
if (ApParSTWarning.Value != SetpointsFrame.Checkbox_SP_Warning.State) {
	ApParSTWarning.Value = SetpointsFrame.Checkbox_SP_Warning.State;
	SetpointsFrame.Checkbox_SP_Warning.isChanged = false;
	AsokuMain.CreateUserMessage("задержки у сигнала " + ParDevPath);
}
if (ApParSTInverted.Value != SetpointsFrame.Checkbox_SP_Inverted.State) {
	ApParSTInverted.Value = SetpointsFrame.Checkbox_SP_Inverted.State;
	SetpointsFrame.Checkbox_SP_Inverted.isChanged = false;
