	if (!ColumnsModel.GetCellData(i,2) != Columns[i].Hidden) return true;
	if (Columns[i].WidthResizeMode == 1) if (ColumnsModel.GetCellData(i,3) != 0) return true;
	if (Columns[i].Width == 0) if (ColumnsModel.GetCellData(i,3) != pAlarms.Main_panel.private.TableView.widthColumn[i]) return true;
	if (Columns[i].Width > 0 && Columns[i].WidthResizeMode == 0) if (ColumnsModel.GetCellData(i,3) != Columns[i].Width) return true;
}

//Поиск изменений в остальных параметрах
if (pAlarms.ActiveMode == 0)
{
	if (Font != pAlarms.Main_tables_Operative.Table_Operative_mode.Colum_1.Font) return true;
	if (FontTitle != pAlarms.Main_tables_Operative.Table_Operative_mode.Colum_1.TitleFont) return true;
	if (RowsHeigth != pAlarms.Main_tables_Operative.Table_Operative_mode.RowHeight) return true;
	if (TitleHeight != pAlarms.Main_tables_Operative.Table_Operative_mode.HeaderHeight) return true;
}
else
{
	if (Font != pAlarms.Main_tables_Historical.Table_History_mode.Colum_1.Font) return true;
	if (FontTitle != pAlarms.Main_tables_Historical.Table_History_mode.Colum_1.TitleFont) return true;
	if (RowsHeigth != pAlarms.Main_tables_Historical.Table_History_mode.RowHeight) return true;
	if (TitleHeight != pAlarms.Main_tables_Historical.Table_History_mode.HeaderHeight) return true;
}

return false;]]>
