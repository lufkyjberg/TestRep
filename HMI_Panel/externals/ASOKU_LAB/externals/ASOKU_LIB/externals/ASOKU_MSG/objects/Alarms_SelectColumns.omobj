	<do access-modifier="private" name="UpdateLayout" display-name="UpdateLayout" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="om">
			<![CDATA[//Debug.Log("UpdateLayout " + String.ToString(Width) + "x" + String.ToString(Height));

cbCount: int4 = 0;
i: int4;
while(i < Checkboxes.Length)
{
	if (Checkboxes[i].Visible)
		cbCount += 1;
	i += 1;
}

if (cbCount > 0)
{	
	cbHeight: double = (Height - 20 - OkButton.Height - 10)/cbCount;
	cbWidth: double = Width - 20;
	cbY: double = 10;
	
	i = 0;
	while(i < Checkboxes.Length)
	{
		if (Checkboxes[i].Visible)
		{		
			Checkboxes[i].Y = cbY;
			Checkboxes[i].Width = cbWidth;
			Checkboxes[i].Height = cbHeight;
			cbY += cbHeight;		
		}
		i += 1;
	}
}

CancelButton.Y = Height - CancelButton.Height - 10;
CancelButton.X = Width - CancelButton.Width - 10;

OkButton.Y = Height - OkButton.Height - 10;
OkButton.X = CancelButton.X - OkButton.Width - 10;]]>
		</body>
	</do>
