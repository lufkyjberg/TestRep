				<do access-modifier="private" name="GetMenuItems" display-name="GetMenuItems" ver="5" type="string" type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d">
					<body kind="javascript">
						<![CDATA[var iconsPath = "Alarms_Icons/";

var items = [];

// режимы
var allowOperative = CurrentEventsAvailable.Value && (!UseSecurity.Value || BoolToken_ViewCurrentEvents.Value);
var allowHistorical = EventHistoryAvailable.Value && (!UseSecurity.Value || BoolToken_ViewEventHistory.Value);
if (allowOperative)
	items.push({ 'id': "operative", 'title': "Оперативный режим", 'icon': iconsPath + "Operative16.png", 'active': ActiveMode != 0 });
if (allowHistorical)
	items.push({ 'id': "historical", 'title': "Исторический режим", 'icon': iconsPath + "Historical16.png", 'active': ActiveMode != 1 });

// квитирование
var allowAcknowledgment = AcknowledgmentAvailable.Value && (UseSecurity.Value ? BoolToken_Acknowledgment.Value : UserName.Value != "");
if (allowAcknowledgment && (ActiveMode == 0 || AllowAddComment.Value))
{
	if (items.length > 0) items.push({ 'title': "-" });
	if (ActiveMode == 0)
	{
		items.push({ 'id': "ack", 'title': "Квитировать", 'icon': iconsPath + "Acknowledge16.png", 'active': !FollowLatestEvent.Value });
		if (AckAllAvailable.Value)
			items.push({ 'id': "ackAll", 'title': "Квитировать все", 'icon': iconsPath + "AcknowledgeAll16.png" });
	}
	if (AllowAddComment.Value)
		items.push({ 'id': "addComment", 'title': "Добавить комментарий...", 'active': ActiveMode != 0 || !FollowLatestEvent.Value });
}

// снимок
var allowSnapshot = SnapshotAvailable.Value && (!UseSecurity.Value || BoolToken_TakeCurrentEventsSnapshot.Value);
if (ActiveMode == 0 && allowSnapshot)
{
	if (items.length > 0) items.push({ 'title': "-" });
	items.push({ 'id': "snapshot",
		'title': ToolBar.SnapshotON.Value ? "Отключить режим снимка" : "Включить режим снимка",
		'icon': iconsPath + (ToolBar.SnapshotON.Value ? "Play16.png" : "Pause16.png") });
}
	
// звуки
var allowSoundManagment = SoundPlaybackManagmentAvailable.Value && (!UseSecurity.Value || BoolToken_SoundPlaybackManagment.Value);
if (ActiveMode == 0 && allowSoundManagment)
{
	if (items.length > 0) items.push({ 'title': "-" });
	items.push({ 'id': "mute",
		'title': Mute.Value ? "Включить проигрывание звуков" : "Отключить проигрывание звуков",
		'icon': iconsPath + (Mute.Value ? "SoundPlay16.png" : "SoundMute16.png") });
	items.push({ 'id': "skipSound", 'title': "Пропустить звук", 'icon': iconsPath + "SoundForward16.png" });
	items.push({ 'id': "clearSoundQueue", 'title': "Очистить очередь звуков", 'icon': iconsPath + "SoundStop16.png" });
}

// отображение событий
var allowClearing =  ActiveMode == 0 && ClearingCurrentEventsAvailable.Value && (UseSecurity.Value ? BoolToken_ClearCurrentEvents.Value : UserName.Value != "");
var allowFiltering = FilterAvailable.Value && (!UseSecurity.Value || BoolToken_Filtering.Value);
var allowSorting = IsFullAccessAllowed.Value && EventTableSortAvailability.Value && (!UseSecurity.Value || BoolToken_EventTableSortAvailability.Value);
if (allowClearing || allowFiltering || allowSorting)
{
	if (items.length > 0) items.push({ 'title': "-" });
	if (allowClearing)
		items.push({ 'id': "clear", 'title': "Очистить список", 'icon': iconsPath + "Clear16.png", 'active': !ToolBar.SnapshotON.Value });
	if (allowFiltering)	
		items.push({ 'id': "viewFilter", 'title': "Фильтр отображения...", 'icon': iconsPath + "Filter16.png" });
	if (allowSorting)
	{
		items.push({ 'id': "defaultSorting", 'title': "Сортировать по умолчанию", 'checked': TableView.defaultSorting.Value, 'active': !TableView.defaultSorting.Value });
		items.push({ 'id': "sort", 'title': "Сортировка...", 'icon': iconsPath + "Sort16.png" });
	}
}

// экспорт
var allowPrint = PrintingAvailable.Value && (!UseSecurity.Value || BoolToken_Printing.Value);
var allowExport = ExportDataAvailability.Value && (!UseSecurity.Value || BoolToken_ExportDataAvailability.Value);
if (allowPrint || allowExport)
{
	var canExport = ActiveMode == 0 ? !OperTableEmpty.Value : !HistTableEmpty.Value && !LoadSaveBusy.Value;
	if (items.length > 0) items.push({ 'title': "-" });
	if (allowPrint)
		items.push({ 'id': "print", 'title': "Печать...", 'icon': iconsPath + "Print16.png",	'active': canExport });
	if (allowExport)
		items.push({ 'id': "export", 'title': "Экспорт...", 'icon': iconsPath + "ExportToFile16.png",	'active': canExport });
}

// вид
if (GUIModificationAvailable.Value)
{
	if (items.length > 0) items.push({ 'title': "-" });
	items.push({ 'id': "toolbar", 'title': "Показывать панель инструментов", 'checked': DisplayToolbar.Value });
	items.push({ 'id': "statusbar", 'title': "Показывать строку состояния", 'checked': DisplayStatePanel.Value });
}

// параметры
if (IsFullAccessAllowed.Value && EditingSettingsAvailable.Value && (!UseSecurity.Value || BoolToken_EditSettings.Value))
{
	if (items.length > 0) items.push({ 'title': "-" });
	items.push({ 'id': "options", 'title': "Параметры...", 'icon': iconsPath + "Options16.png" });
}

return JSON.stringify(items);]]>
					</body>
				</do>
				<object access-modifier="private" name="OnMenuClick" display-name="OnMenuClick" uuid="c8a27721-dcb8-47f1-abc8-950ee8a8ac97" base-type="Action" base-type-id="a9ee9770-1c4a-44c9-b815-157d9fc2ab95" ver="5">
