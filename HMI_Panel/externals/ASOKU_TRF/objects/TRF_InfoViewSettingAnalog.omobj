		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="OnTimer">
			<body kind="om">
				<![CDATA[//таймер, задающий параметры при инициализации формы


if (ApParID.Value == "" || ApParID.Value == "0")  Button_Settings.Enabled = false;
else  Button_Settings.Enabled = true;


IDFrame.TextEdit_ID.Text = ApParID.ValueAsString;
IDFrame.TextEdit_UpdateTime.Text = DateTime.ToString(GetTimeTransferState());


if (isNeedUpdateSettings() == false) IDFrame.Image_NeedUpdate.Visible = false; 
else IDFrame.Image_NeedUpdate.Visible = true;  //проверка актуальности 


Timer_Init.Active = false;]]>
			</body>
		</do-on>
