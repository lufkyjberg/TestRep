				<do access-modifier="private" name="GetDescription" display-name="GetDescription" ver="5" type="string" type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d">
					<body kind="javascript">
						<![CDATA[var columns = JSON.parse(ViewFilter.GetColumns());
function MakeDescr(jsonstr)
{
	var json = JSON.parse(jsonstr);
	var text = "";
	function AddDescr(title,value)
	{
		if (value!=undefined && value.length > 0)
		{
			if (text.length>0) text += ", ";
			text += title + ": " + value;
		}
	}
	for (let c = 0; c < columns.length; c++)
	{
		var id = columns[c];
		var value = json[id];
		if (id != Main_tables_Operative.Table_Operative_mode.Colum_7.Id || value != "Все")
			AddDescr(GetColTitle(id),value);
	}
	return text;
}

var descr = "";
if (JsonFilter.used_filters == "")
{
	var _obj = {'time':JsonFilter.time, 'active_time':JsonFilter.active_time, 'deactive_time':JsonFilter.deactive_time, 'severity':JsonFilter.severity, 'group':JsonFilter.group, 'message':JsonFilter.message, 'ack':JsonFilter.ack, 'ack_time':JsonFilter.ack_time, 'actor_id':JsonFilter.actor_id, 'ack_comment':JsonFilter.ack_comment};
	_obj[Main_tables_Operative.Table_Operative_mode.Colum_5.Id] = JsonFilter.source;
	descr = MakeDescr(JSON.stringify(_obj));
}
else
	descr = MakeDescr(JsonFilter.UsedFiltersCombine());
return descr;]]>
