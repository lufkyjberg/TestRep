				<![CDATA[// Применение измененных значений


if(Rectangle_Catalogs.Checkbox_Source1.FontColor == AsokuColor.asokuSky){
	if (Rectangle_Catalogs.Checkbox_Source1.State) Ap_Par_PrioritySource1.Value = 1;
	else Ap_Par_PrioritySource1.Value = 0;
}
if(Rectangle_Catalogs.Checkbox_Source2.FontColor == AsokuColor.asokuSky){
	if (Rectangle_Catalogs.Checkbox_Source2.State) Ap_Par_PrioritySource2.Value = 1;
	else Ap_Par_PrioritySource2.Value = 0;
}
if(Rectangle_Catalogs.Checkbox_Source3.FontColor == AsokuColor.asokuSky){
	if (Rectangle_Catalogs.Checkbox_Source3.State) Ap_Par_PrioritySource3.Value = 1;
	else Ap_Par_PrioritySource3.Value = 0;
}
if(Rectangle_Catalogs.Checkbox_Source4.FontColor == AsokuColor.asokuSky){
	if (Rectangle_Catalogs.Checkbox_Source4.State) Ap_Par_PrioritySource4.Value = 1;
	else Ap_Par_PrioritySource4.Value = 0;
}
if(Rectangle_Catalogs.Checkbox_Source5.FontColor == AsokuColor.asokuSky){
	if (Rectangle_Catalogs.Checkbox_Source5.State) Ap_Par_PrioritySource5.Value = 1;
	else Ap_Par_PrioritySource5.Value = 0;
}
if(Rectangle_Catalogs.Checkbox_Source6.FontColor == AsokuColor.asokuSky){
	if (Rectangle_Catalogs.Checkbox_Source6.State) Ap_Par_PrioritySource6.Value = 1;
	else Ap_Par_PrioritySource6.Value = 0;
}


//вытаскиваем значение итема выпадающего списка по выбранному индексу(с помощью GetItem), преобразуем в uint2 и записываем на сервер(если преобразование успешно (число "1,2..") - оно и запишется, если неуспешно (текст "Искл." - запишется 0, что соответсвует логике в ДЕВ) 
if(Rectangle_Catalogs.ComboBox_Priority_Source_1.FontColor == AsokuColor.asokuSky) Ap_Par_PrioritySource1.Value = String.ToUint2(Rectangle_Catalogs.ComboBox_Priority_Source_1.GetItem(Rectangle_Catalogs.ComboBox_Priority_Source_1.SelectedIndex), 0);
if(Rectangle_Catalogs.ComboBox_Priority_Source_2.FontColor == AsokuColor.asokuSky) Ap_Par_PrioritySource2.Value = String.ToUint2(Rectangle_Catalogs.ComboBox_Priority_Source_2.GetItem(Rectangle_Catalogs.ComboBox_Priority_Source_2.SelectedIndex), 0);
if(Rectangle_Catalogs.ComboBox_Priority_Source_3.FontColor == AsokuColor.asokuSky) Ap_Par_PrioritySource3.Value = String.ToUint2(Rectangle_Catalogs.ComboBox_Priority_Source_3.GetItem(Rectangle_Catalogs.ComboBox_Priority_Source_3.SelectedIndex), 0);
if(Rectangle_Catalogs.ComboBox_Priority_Source_4.FontColor == AsokuColor.asokuSky) Ap_Par_PrioritySource4.Value = String.ToUint2(Rectangle_Catalogs.ComboBox_Priority_Source_4.GetItem(Rectangle_Catalogs.ComboBox_Priority_Source_4.SelectedIndex), 0);
if(Rectangle_Catalogs.ComboBox_Priority_Source_5.FontColor == AsokuColor.asokuSky) Ap_Par_PrioritySource5.Value = String.ToUint2(Rectangle_Catalogs.ComboBox_Priority_Source_5.GetItem(Rectangle_Catalogs.ComboBox_Priority_Source_5.SelectedIndex), 0);
if(Rectangle_Catalogs.ComboBox_Priority_Source_6.FontColor == AsokuColor.asokuSky) Ap_Par_PrioritySource6.Value = String.ToUint2(Rectangle_Catalogs.ComboBox_Priority_Source_6.GetItem(Rectangle_Catalogs.ComboBox_Priority_Source_6.SelectedIndex), 0);



	//Устанавливаем режим (с учетом ЧекБокса отвечающего за режим Среднее значение без учета качества)
if (ComboBox_SelectMode.SelectedIndex == 0 && !(Checkbox_ModeAvarageValueWithoutQiality.State)) Ap_Par_SelectModeBytes.Value = 1;	
else if (ComboBox_SelectMode.SelectedIndex == 0 && Checkbox_ModeAvarageValueWithoutQiality.State) Ap_Par_SelectModeBytes.Value = 2;
else if (ComboBox_SelectMode.SelectedIndex == 1) Ap_Par_SelectModeBytes.Value = 4;	
else if (ComboBox_SelectMode.SelectedIndex == 2) Ap_Par_SelectModeBytes.Value = 8;                                    


if((Rectangle_Catalogs.Checkbox_Source1.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.Checkbox_Source2.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.Checkbox_Source3.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.Checkbox_Source4.FontColor == AsokuColor.asokuSky) ||  (Rectangle_Catalogs.Checkbox_Source5.FontColor == AsokuColor.asokuSky) ||  (Rectangle_Catalogs.Checkbox_Source6.FontColor == AsokuColor.asokuSky) || (ComboBox_SelectMode.FontColor == AsokuColor.asokuSky) || (Checkbox_ModeAvarageValueWithoutQiality.FontColor  == AsokuColor.asokuSky) || (Rectangle_Catalogs.ComboBox_Priority_Source_1.FontColor == AsokuColor.asokuSky) ||(Rectangle_Catalogs.ComboBox_Priority_Source_2.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.ComboBox_Priority_Source_3.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.ComboBox_Priority_Source_4.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.ComboBox_Priority_Source_5.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.ComboBox_Priority_Source_6.FontColor == AsokuColor.asokuSky))
{
  TakeMessage();                                        // выдача сообщения об изменениях пользователя 
  SetColorField();                                      // установка первоначального цвета шрифта у полей
  Button_Acc.FontColor = AsokuColor.asokuWhite;    // сброс цвета символов у кнопки Применить
}  

      ]]>
