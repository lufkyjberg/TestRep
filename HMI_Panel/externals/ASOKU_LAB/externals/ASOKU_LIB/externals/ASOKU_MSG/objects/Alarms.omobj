					<body kind="javascript">
						<![CDATA[// формируем коллекцию столбцов
var columns = [];
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_1);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_2);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_3);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_4);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_5);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_6);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_7);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_8);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_9);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_10);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_14);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_17);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_19);
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_20);
if (TableView.enable2122Columns)
{
	columns.push(Main_tables_Operative.Table_Operative_mode.Colum_21);
	columns.push(Main_tables_Operative.Table_Operative_mode.Colum_22);
}
columns.push(Main_tables_Operative.Table_Operative_mode.Colum_23);

// формируем коллекцию элементов меню
var items = [];
var i = 0;
while (i < columns.length)
{
	var column = columns[i];
	
	// для некоторых столбцов определяем активность команды меню
	var active = true;	
	if (i == 0) // Colum_1
		active = !Main_tables_Operative.Table_Operative_mode.Colum_2.Hidden || Main_tables_Operative.Table_Operative_mode.Colum_1.Hidden;
	else if (i == 1) // Colum_2
		active = !Main_tables_Operative.Table_Operative_mode.Colum_1.Hidden || Main_tables_Operative.Table_Operative_mode.Colum_2.Hidden;
	else if (i == 3) // Colum_4
		active = Main_tables_Operative.Table_Operative_mode.Colum_4.Hidden;
	
	items.push({
		 'id': column.GetIndex(),
		 'title': column.Title == "" ? column.Description : column.Title,
		 'checked': !column.Hidden,
		 'staysOpen': true,
 		 'active': active,
		});

	i += 1;
}

// сортируем элементы меню в соответствии с порядком отображения в таблице
items = items.sort((a, b) => a.id - b.id);

return JSON.stringify(items);]]>
