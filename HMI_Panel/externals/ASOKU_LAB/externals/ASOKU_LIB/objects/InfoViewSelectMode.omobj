	<do access-modifier="private" name="TakeMessage" display-name="  Функция, которая выдает сообщение об изменениях пользователя" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="om">
			<![CDATA[//выдача сообщений об изменениях пользователя

//смена режима
if(ComboBox_SelectMode.FontColor == AsokuColor.asokuSky){
	if(ComboBox_SelectMode.SelectedIndex == ComboBox_SelectMode.IndexOf("Среднее значение")){
		if(Checkbox_ModeAvarageValueWithoutQiality.State == false && Checkbox_ModeAvarageValueWithoutQiality.FontColor == AsokuColor.asokuWhite) AsokuMain.CreateUserMessage("расчета результирующего значения ''" + Ap_Par_Name.ValueAsString + "''. Установил режим: Среднее значение " + Ap_Source_Par.Path); 
		else if(Checkbox_ModeAvarageValueWithoutQiality.State == true && Checkbox_ModeAvarageValueWithoutQiality.FontColor == AsokuColor.asokuSky) AsokuMain.CreateUserMessage("расчета результирующего значения ''" + Ap_Par_Name.ValueAsString + "''. Установил режим: Среднее значение без учета качества сигнала " + Ap_Source_Par.Path);
	} 
	else if(ComboBox_SelectMode.SelectedIndex ==  ComboBox_SelectMode.IndexOf("Выбор пользователя")) AsokuMain.CreateUserMessage("расчета результирующего значения ''" + Ap_Par_Name.ValueAsString + "''. Установил режим: Выбор пользователя " + Ap_Source_Par.Path);
	else if(ComboBox_SelectMode.SelectedIndex ==  ComboBox_SelectMode.IndexOf("По приоритету")) AsokuMain.CreateUserMessage("расчета результирующего значения ''" + Ap_Par_Name.ValueAsString + "''. Установил режим: По приоритету " + Ap_Source_Par.Path);
}

//Если меняем режим с "Среднего значение" на "Срденее значение без учета качества" и наоборот. В это случае в выпадающем списке изменений не будет - только смена состояния чекбокса "без учета качества"), поэтому учитываем эту ситуацию отдельным условием
else if (Checkbox_ModeAvarageValueWithoutQiality.FontColor == AsokuColor.asokuSky){
	if(Checkbox_ModeAvarageValueWithoutQiality.State) AsokuMain.CreateUserMessage("расчета результирующего значения ''" + Ap_Par_Name.ValueAsString + "''. Установил режим: Среднее значение без учета качества сигнала " + Ap_Source_Par.Path);
	else AsokuMain.CreateUserMessage("расчета результирующего значения ''" + Ap_Par_Name.ValueAsString + "''. Установил режим: Среднее значение " + Ap_Source_Par.Path);
}


if((Rectangle_Catalogs.Checkbox_Source1.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.Checkbox_Source2.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.Checkbox_Source3.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.Checkbox_Source4.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.Checkbox_Source5.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.Checkbox_Source6.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.ComboBox_Priority_Source_1.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.ComboBox_Priority_Source_2.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.ComboBox_Priority_Source_3.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.ComboBox_Priority_Source_4.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.ComboBox_Priority_Source_5.FontColor == AsokuColor.asokuSky) || (Rectangle_Catalogs.ComboBox_Priority_Source_6.FontColor == AsokuColor.asokuSky)) AsokuMain.CreateUserMessage("расчета результирующего значения ''" + Ap_Par_Name.ValueAsString + "''. " + Ap_Source_Par.Path);




]]>
		</body>
	</do>
