	<do access-modifier="private" name="isNeedUpdateSettings" display-name="isNeedUpdateSettings" ver="5" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
		<body kind="javascript">
			<![CDATA[// Функция проверки актуальности данных сигнала

let isNeedUpdate = false;

var tooltip; //всплывающая подсказка


if(TRF_Base.Ap_Par_ID.Value != "")
{
  if( (TRF_Base.Ap_Par_transferState.Advised == true) && (TRF_Base.Ap_Par_transferState.Value != ""))
  {
    var transferStates  = JSON.parse(TRF_Base.Ap_Par_transferState.Value); 
    var transferState  = transferStates[0];   
    
    tooltip = "Несоответствия в следующих полях: ";	 
	 if(TRF_Base.Ap_Par_ST_InvertedOn !=  transferState.TypeInverted)
    {
    	   tooltip  += "\n *статус канала инвертирования" ;  
 			isNeedUpdate = true;
	 }
	         
    if(TRF_Base.isUpdateTime(transferState.TransferStateTime)) 
    {
   		tooltip  += "\n *превышено время актуальности"         
     		isNeedUpdate = true;  
    }
    
    if (isNeedUpdate == false ) tooltip = "";
    
    Rectangle_7.Image_NeedUpdate.Tooltip = tooltip;     
    return isNeedUpdate;
  }
  else if (TRF_Base.Ap_Par_transferState.Value == "") 
  { 
      tooltip  = "Нет сохраненных значений трансфер-блока";
      Rectangle_7.Image_NeedUpdate.Tooltip = tooltip;     
  	 	return true; //трансфер ранее не считывался
  }
}

else return false;]]>
