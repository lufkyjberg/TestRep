	<do access-modifier="private" name="isNeedUpdateSettings" display-name="isNeedUpdateSettings" ver="5" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
		<body kind="om">
			<![CDATA[// Функция проверки актуальности данных сигнала

if(ApParID.Value != "")
{
  if( (ApParTransferState.Advised == true) && (ApParTransferState.Value != ""))
  {
   id: string = ApParID.Value;
   type: string = String.SubString(id, String.IndexOf(id, ":")+1);

 	if(type == "AI") return isNeedUpdateSettingsAI(); 
	else if (type == "AO") return isNeedUpdateSettingsAO(); 
	else if (type == "AI_SVZ") return isNeedUpdateSettingsAI(); 
	else if(type == "fAI") return isNeedUpdateSettingsfAI(); 
  
  }
  else if (ApParTransferState.Value == "") 
  { 
      IDFrame.Image_NeedUpdate.Tooltip = "Нет сохраненных значений трансфер-блока";     
  	 	return true; //трансфер ранее не считывался
  }
}

else return false;]]>
		</body>
	</do>
