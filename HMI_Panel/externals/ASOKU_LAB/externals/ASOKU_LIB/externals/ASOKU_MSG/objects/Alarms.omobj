	sorting.Columns.push({"Colum_23":columns[14]});

	File.WriteTextFile(sortingPath, JSON.stringify(sorting, null, 2));
}
catch(error)
{
	log("Не удалось сохранить в "  + sortingPath + ": " + error);
}]]>
