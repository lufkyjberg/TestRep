			<![CDATA[
TRF_Base.parDevPath = here.ParDevPath.Value; //задание пути параметра


//задание положения окна
posWindowNum = 0; 
while(posWindowNum<= AsokuMain.InfoTabViewPosNumber.Length && AsokuMain.InfoTabViewPosNumber[posWindowNum] == true )
{
  posWindowNum += 1;
}
if(posWindowNum == 1)
{
  this.WindowX = 400;
  this.WindowY = 10;
}
if(posWindowNum == 2)
{
  this.WindowX = 800;
  this.WindowY = 10;
}
if(posWindowNum == 3)
{
  this.WindowX = 1200;
  this.WindowY = 10;
}
if(posWindowNum == 4)
{
  this.WindowX = 400;
  this.WindowY = 500;
}
if(posWindowNum == 5)
{
  this.WindowX = 800;
  this.WindowY = 500;
}
if(posWindowNum == 6)
{
  this.WindowX = 1200;
  this.WindowY = 500;
}



Button_Acc.Enabled = false;
Button_Refresh.Enabled = false;

Rectangle_1.ComboBox_ExternalMode.AddItem("Обычный");
Rectangle_1.ComboBox_ExternalMode.AddItem("Симуляция");


TRF_Base.constructor(""); 
Timer_Init.Active = true;     //выполняется код в таймере


]]>
