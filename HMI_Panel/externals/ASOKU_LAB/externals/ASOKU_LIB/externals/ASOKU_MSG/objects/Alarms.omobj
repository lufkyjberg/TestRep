	infFile = File.CombinePath(path, "alarms.inf");
	folder = path;
	//File.CreateRecursiveFolder(folder);//возможно это забота внешнего кода
}

if (!File.Exists(folder))
{
	EventParams[4] = "Папка "+path+" отсутствует";
