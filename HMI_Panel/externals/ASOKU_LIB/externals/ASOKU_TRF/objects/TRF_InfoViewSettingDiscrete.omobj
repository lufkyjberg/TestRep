	<do access-modifier="private" name="isNeedUpdateSettings" display-name="isNeedUpdateSettings" ver="5" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
		<body kind="javascript">
			<![CDATA[// Функция проверки актуальности данных сигнала

let isNeedUpdate = false;
var tooltip; //всплывающая подсказка


if(ApParID.Value != "")
{
if( (ApParTransferState.Advised == true) && (ApParTransferState.Value != ""))
{
    var transferStates  = JSON.parse(ApParTransferState.Value); 
    var transferState  = transferStates[0];   
    
 	 tooltip = "Несоответствия в следующих полях: ";
    if(ApParST_AlarmOn.Value !=  transferState.Alarm)
    {
    	   tooltip  += "\n *значение аварийной сигнализации" ;  
 			isNeedUpdate = true;
	 }
	 
	 if(ApParST_WarningOn.Value !=  transferState.Warning)
    {
    	   tooltip  += "\n *значение предупредительной сигнализации" ;  
 			isNeedUpdate = true;
	 }
	 
	 if(ApParST_InvertedOn.Value !=  transferState.TypeInverted)
    {
    	   tooltip  += "\n *статус канала инвертирования" ;  
 			isNeedUpdate = true;
	 }
	         
    if( isUpdateTime(transferState.TransferStateTime)) 
    {
   		tooltip  += "\n *превышено время актуальности"         
     		isNeedUpdate = true;  
   } 
   
   
    if(isUpdateTime(transferState.TransferStateTime)) 
    {
   		tooltip  += "\n *превышено время актуальности"         
     		isNeedUpdate = true;  
    }
    
    if (isNeedUpdate == false ) tooltip = "";
    
    Rectangle_4.Image_NeedUpdate.Tooltip = tooltip;     
    return isNeedUpdate;
}   
else if (ApParTransferState.Value == "") 
{ 
      tooltip  = "Нет сохраненных значений трансфер-блока";
      Rectangle_4.Image_NeedUpdate.Tooltip = tooltip;     
  	 	return true; //трансфер ранее не считывался
}	
}

else return false;
]]>
		</body>
	</do>
	<do access-modifier="private" name="isUpdateTime" display-name="isUpdateTime" ver="5" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
		<body kind="om">
			<![CDATA[//Функция проверки необходимости обновления трансфер-блока по времени
//возвращает true, если данные необходимо обновить
if(DateTime.Now() - transferStateTime <= AsokuConst.TimeActualSettings) return false;
else return true;
    ]]>
		</body>
		<param access-modifier="private" name="transferStateTime" display-name="" uuid="1a22cd15-bb05-410c-a4a0-5760b01b5f6d" base-type="uint8" base-type-id="c8ff7701-da6e-4d53-888a-81fa680ff561" ver="5"/>
	</do>
	<do access-modifier="private" name="GetTimeTransferState" display-name="GetTimeTransferState" ver="5" type="uint8" type-id="c8ff7701-da6e-4d53-888a-81fa680ff561">
		<body kind="javascript">
			<![CDATA[
if(ApParTransferState.Advised == true && ApParTransferState.Value != "")
{
  var transferStates  = JSON.parse(ApParTransferState.Value); 
  var transferState  = transferStates[0];     
  return  transferState.TransferStateTime;
