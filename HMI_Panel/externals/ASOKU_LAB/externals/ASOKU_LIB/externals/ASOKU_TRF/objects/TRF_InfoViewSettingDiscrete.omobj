		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="Invoked">
			<body kind="om">
				<![CDATA[// Применение измененных значений                                                       
if(Checkbox_SimOn.FontColor ==AsokuColor.asokuSky) Ap_Par_SimOn.Value = Checkbox_SimOn.State;
if(TextEdit_Value.FontColor ==AsokuColor.asokuSky) Ap_Par_SimValue.Value = String.ToUint1(TextEdit_Value.Text,0) > 0;
if(TextEdit_SimState.FontColor ==AsokuColor.asokuSky) Ap_Par_SimStatus.Value = String.ToUint2(TextEdit_SimState.Text,0);
if(TextEdit_DelayLimits.FontColor ==AsokuColor.asokuSky) Ap_Par_DelayLimits.Value = String.ToUint4(TextEdit_DelayLimits.Text,1000);
if(TextEdit_SuppressionTimeLimit.FontColor ==AsokuColor.asokuSky) Ap_Par_SuppressionTimeLimit.Value = String.ToUint4(TextEdit_SuppressionTimeLimit.Text,1000);

if(Checkbox_SimOn.FontColor ==AsokuColor.asokuSky)       Ap_Par_SimOn.Value = Checkbox_SimOn.State;
if(Checkbox_SP_Alarm.FontColor ==AsokuColor.asokuSky)    Ap_Par_ST_AlarmOn.Value = Checkbox_SP_Alarm.State;
if(Checkbox_SP_Warning.FontColor ==AsokuColor.asokuSky)  Ap_Par_ST_WarningOn.Value = Checkbox_SP_Warning.State;
if(Checkbox_SP_Inverted.FontColor ==AsokuColor.asokuSky) Ap_Par_ST_InvertedOn.Value = Checkbox_SP_Inverted.State;

 if((TextEdit_Value.FontColor ==AsokuColor.asokuSky)  ||(TextEdit_SimState.FontColor ==AsokuColor.asokuSky) || (TextEdit_DelayLimits.FontColor ==AsokuColor.asokuSky) || (TextEdit_SuppressionTimeLimit.FontColor ==AsokuColor.asokuSky)
||(Checkbox_SimOn.FontColor ==AsokuColor.asokuSky) || (Checkbox_SP_Alarm.FontColor ==AsokuColor.asokuSky) ||(Checkbox_SP_Warning.FontColor ==AsokuColor.asokuSky) || (Checkbox_SP_Inverted.FontColor ==AsokuColor.asokuSky)) 
{
  TakeMessage(); // выдача сообщения об изменениях пользователя //
  SetColorField();                            // установка первоначального цвета шрифта у полей
  Button_Acc.FontColor =AsokuColor.asokuWhite;  // сброс цвета символов у кнопки Применить
} ]]>
			</body>
		</do-on>
	</object>
