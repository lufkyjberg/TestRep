		else Ap_Par_PrioritySources[i].Value = 99;
	}
	if(Rectangle_Catalogs.ComboBox_Priority_Sources[i].FontColor == AsokuColor.asokuSky){ Ap_Par_PrioritySources[i].Value =  (Rectangle_Catalogs.ComboBox_Priority_Sources[i].GetItem(Rectangle_Catalogs.ComboBox_Priority_Sources[i].SelectedIndex) != "Искл.")?(Rectangle_Catalogs.ComboBox_Priority_Sources[i].GetItem(Rectangle_Catalogs.ComboBox_Priority_Sources[i].SelectedIndex)):(99);
	//String.ToUint2(Rectangle_Catalogs.ComboBox_Priority_Sources[i].GetItem(Rectangle_Catalogs.ComboBox_Priority_Sources[i].SelectedIndex), 99); //вытаскиваем значение итема выпадающего списка по выбранному индексу(с помощью GetItem), преобразуем в uint2 и записываем на сервер(если преобразование успешно (число "1,2..") - оно и запишется, если неуспешно (текст "Искл." - запишется 99, что соответсвует логике в ДЕВ) 
	}
}


	//Устанавливаем режим (с учетом ЧекБокса отвечающего за режим Среднее значение без учета качества)
if (ComboBox_SelectMode.SelectedIndex == 0 && !(Checkbox_ModeAvarageValueWithoutQiality.State)) Ap_Par_SelectModeBytes.Value = 1;	
else if (ComboBox_SelectMode.SelectedIndex == 0 && Checkbox_ModeAvarageValueWithoutQiality.State) Ap_Par_SelectModeBytes.Value = 2;
else if (ComboBox_SelectMode.SelectedIndex == 1) Ap_Par_SelectModeBytes.Value = 4;	
else if (ComboBox_SelectMode.SelectedIndex == 2) Ap_Par_SelectModeBytes.Value = 8;                                    

for (let i = 0; i < 6; i ++){
	if (Rectangle_Catalogs.Checkbox_Sources[i].FontColor == AsokuColor.asokuSky || Rectangle_Catalogs.ComboBox_Priority_Sources[i].FontColor == AsokuColor.asokuSky || ComboBox_SelectMode.FontColor == AsokuColor.asokuSky || Checkbox_ModeAvarageValueWithoutQiality.FontColor == AsokuColor.asokuSky){
		TakeMessage();                                        // выдача сообщения об изменениях пользователя 
		SetColorField();                                      // установка первоначального цвета шрифта у полей
		Button_Acc.FontColor = AsokuColor.asokuWhite;    // сброс цвета символов у кнопки Применить
		break;
	}  
}	
	


