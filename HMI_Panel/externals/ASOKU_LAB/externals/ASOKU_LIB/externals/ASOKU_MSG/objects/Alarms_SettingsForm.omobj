isDefaultColumns: bool = true;
i: int4 = 0;
while (i < ColumnsModel.GetRowCount())
{
	id: string = Variant.ToString(ColumnsModel.GetCellData(i,6), "");
	if (Variant.ToString(ColumnsModel.GetCellData(i,1), "") != DefaultColumnTitle(id) ||
		Variant.ToBool(ColumnsModel.GetCellData(i,2), false) != DefaultColumnVisible(id) ||
		Variant.ToString(ColumnsModel.GetCellData(i,3), "") != String.ToString(DefaultColumnWidth(id)) ||
		Variant.ToInt4(ColumnsModel.GetCellData(i,4), 0) != DefaultColumnIndex(id))
	{
		isDefaultColumns = false;
		break;
	}
	
	i +=1;
}

IsDefault = isDefaultColumns &&
