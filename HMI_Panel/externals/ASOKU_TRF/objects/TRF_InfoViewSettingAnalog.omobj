	<do access-modifier="private" name="GetTimeTransferState" display-name="GetTimeTransferState" ver="5" type="uint8" type-id="c8ff7701-da6e-4d53-888a-81fa680ff561">
		<body kind="javascript">
			<![CDATA[
if(ApParTransferState.Advised == true && ApParTransferState.Value != "")
{
  var transferStates  = JSON.parse(ApParTransferState.Value); 
  var transferState  = transferStates[0];     
  return  transferState.TransferStateTime;
}
return 0;]]>
		</body>
	</do>
