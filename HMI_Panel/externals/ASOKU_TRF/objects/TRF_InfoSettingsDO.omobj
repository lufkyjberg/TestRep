TRF_Base.NumChan = String.ToFloat( Rectangle_1.LineEdit_NumChannel.Text,0);    // номер канала

TRF_Base.StaticOff =  Rectangle_1.Checkbox_StaticOff.State ;               // канал отключен
TRF_Base.TypeInverted = Rectangle_1.Checkbox_TypeInverted.State; 		   	// канал инвертирован
TRF_Base.DisableCheckOOS =  Rectangle_1.Checkbox_DisableCheckOOS.State ;   // отключение диагностики
TRF_Base.NoMes =  Rectangle_1.Checkbox_NoMes.State ;                       // отключение сообщений
TRF_Base.SimulationValue = Rectangle_1.Checkbox_SimulationValue.State;		// значение в режиме симуляции

// Задержки 
TRF_Base.Latency = String.ToFloat(Rectangle_2.LineEdit_Latency.Text,0);   // задержка             
TRF_Base.DelayStateOOSOn = String.ToFloat(Rectangle_2.LineEdit_DelayStateOOSOn.Text,0);   // задержка неисправности (приход события)
TRF_Base.DelayStateOOSOff = String.ToFloat(Rectangle_2.LineEdit_DelayStateOOSOff.Text,0); // задержка неисправности (уход события)


]]>
