	<do access-modifier="private" name="isNeedUpdateSettings" display-name="isNeedUpdateSettings" ver="5" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
		<body kind="javascript">
			<![CDATA[// Функция проверки актуальности данных сигнала

let isNeedUpdate = false;
var tooltip; //всплывающая подсказка


if(Ap_Par_ID.Value != 0)
{
  if( (Ap_Par_TransferState.Advised == true) && (Ap_Par_TransferState.Value != ""))
  {
    var transferStates  = JSON.parse(Ap_Par_TransferState.Value); 
    var transferState  = transferStates[0];   
    
    tooltip = "Несоответствия в следующих полях: ";
    if(Ap_Par_SP_HH_value.Value !=  transferState.HH_value)
    {
    	   tooltip  += "\n *значение верхн. аварийной сигнализации" ;  
 			isNeedUpdate = true;
	 }
    if(Ap_Par_SP_HH_on.Value == transferState.HH_off)
    {
      	tooltip  += "\n *статус верхн. аварийной сигнализации" ;  
       	isNeedUpdate = true;       
    }
  
    if(Ap_Par_SP_H_value.Value !=  transferState.H_value)
    {
			tooltip  += "\n *значение верхн. предупредительной сигнализации";       		
     		isNeedUpdate= true;
    }
    if(Ap_Par_SP_H_on.Value == transferState.H_off)
    {
     		tooltip  += "\n *статус верхн. предупредительной сигнализации"  ; 
     		isNeedUpdate = true;
    }
  
    if(Ap_Par_SP_L_value.Value !=  transferState.L_value)
    {
      	tooltip  += "\n *значение нижн. предупредительной сигнализации";
      	isNeedUpdate = true;
    }
    if(Ap_Par_SP_L_on.Value == transferState.L_off)
    {
   	 	tooltip  += "\n *статус нижн. предупредительной сигнализации" ;
     		isNeedUpdate = true;
    }
  
    if(Ap_Par_SP_LL_value.Value !=  transferState.LL_value)
    {
    	 	tooltip  += "\n *значение нижн. аварийной сигнализации";
    		isNeedUpdate = true;
    }
    if(Ap_Par_SP_LL_on.Value == transferState.LL_off)
    {
		   tooltip  += "\n *статус нижн. аварийной сигнализации" ;
    		isNeedUpdate = true;
    }
  
    if(Ap_Par_MinValue != transferState.PhysRangeMin)
    {
    		tooltip  += "\n *минимальное значение";
    	   isNeedUpdate = true;
    }
    if(Ap_Par_MaxValue != transferState.PhysRangeMax)
    {
    		tooltip  += "\n *максимальное значение" ;
    	   isNeedUpdate = true;
    }
  
    if(Ap_Par_ID.Value != transferState.ID)
    {
   	 	tooltip  += "\n *ID" ;
    	 	isNeedUpdate = true;  
    }
      
    if(isUpdateTime(transferState.TransferStateTime)) 
    {
   		tooltip  += "\n *превышено время актуальности"         
     		isNeedUpdate = true;  
    }
    
