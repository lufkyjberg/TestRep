	<do access-modifier="private" name="ReadElements" display-name="ReadElements" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="om">
			<![CDATA[//Функция заполнения значений из типа Трансфера

NORMAL_MODE: int4     = 0;
EXTERNAL_MODE: int4   = 1;
SIMULATION_MODE: int4 = 2;



// Общее              
Rectangle_1.LineEdit_SignalId.Text = String.ToString(TRF_Base.ID);           // id настраиваемого сигнала
Rectangle_1.LineEdit_NumRack.Text = String.ToString( TRF_Base.NumRack);      // номер крейта
Rectangle_1.LineEdit_NumModule.Text  = String.ToString( TRF_Base.NumModul);  // номер модуля
Rectangle_1.LineEdit_NumChannel.Text = String.ToString(TRF_Base.NumChan);    // номер канала

if(TRF_Base.SimulationMode)                                 // режим
    Rectangle_1.ComboBox_ExternalMode.SelectedIndex = SIMULATION_MODE;
else
    Rectangle_1.ComboBox_ExternalMode.SelectedIndex = NORMAL_MODE;
    

Rectangle_1.Checkbox_StaticOff.State = TRF_Base.StaticOff;                            // канал отключен
Rectangle_1.Checkbox_DisableCheckOOS.State = TRF_Base.DisableCheckOOS;                // отключение диагностики
Rectangle_1.Checkbox_NoMes.State = TRF_Base.NoMes;                                    // отключение сообщений

Rectangle_1.LineEdit_SimulationValue.Text= String.ToString(TRF_Base.SimulationValue); // значение в режиме симуляции

              
// Задержки        
Rectangle_2.LineEdit_DelayStateOOSOn.Text  = String.ToString(TRF_Base.DelayStateOOSOn);  // задержка неисправности (приход события)
Rectangle_2.LineEdit_DelayStateOOSOff.Text = String.ToString(TRF_Base.DelayStateOOSOff); // задержка неисправности (уход события)

// Масштабирование
Rectangle_3.LineEdit_PhysRangeMax.Text= String.ToString(TRF_Base.PhysRangeMax);  // максимальное значение физической величины
Rectangle_3.LineEdit_PhysRangeMin.Text= String.ToString(TRF_Base.PhysRangeMin);  // минимальное значение физической величины



if(isNeedUpdateSettings() == true) Rectangle_7.Image_NeedUpdate.Visible = true;
else Rectangle_7.Image_NeedUpdate.Visible = false;




]]>
		</body>
	</do>
