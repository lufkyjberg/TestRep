	<object access-modifier="private" name="RerenderTimer" display-name="RerenderTimer" uuid="0ac1dd7a-1a08-4631-8f69-835efe65a14a" base-type="Timer" base-type-id="72ba7cd4-549e-41f2-923c-2431b91f349a" ver="5" description="" cardinal="1">
		<designed target="Period" value="50" ver="5"/>
		<designed target="Active" value="true" ver="5"/>
		<do-on access-modifier="private" name="RerenderHndlr" display-name="RerenderHndlr" ver="5" event="OnTimer">
			<body kind="om">
				<![CDATA[if (ApParValue.Advised && Value.Text == "***") {
	// here.PenColor
	
	if (ApParStatusBytes.Value == AsokuConst.StatusBytes.Changed) here.PenColor = AsokuColor.asokuCyan;
	else here.PenColor = AsokuColor.asokuTransparent;
	
	// Value.Text
	
	if (ApParValue.Advised) Value.Text = String.ToString(ApParDisplayFormat.Value, ApParValue.Value);
	
	// Value.FontColor
	
	if (!ApParValue.Advised || ApParStatusBytes.Value == AsokuConst.StatusBytes.Invalid) Value.FontColor = AsokuColor.asokuBlack;
	else if (ApParAlarmBytes.Value == AsokuConst.AlarmBytes.Alarm || ApParAlarmBytes.Value == AsokuConst.AlarmBytes.WarningAndAlarm)	 Value.FontColor = AsokuColor.asokuRed;
	else if (ApParAlarmBytes.Value == AsokuConst.AlarmBytes.Warning) Value.FontColor = AsokuColor.asokuYellow;
	else Value.FontColor = AsokuColor.asokuGreen;
	
	// Units.Visible
	
	Units.Visible = !HideUnits;
	
	// Units.FontColor
	
	Units.FontColor = AsokuColor.asokuWhite;
	
	//this.Active = false;
}]]>
			</body>
		</do-on>
