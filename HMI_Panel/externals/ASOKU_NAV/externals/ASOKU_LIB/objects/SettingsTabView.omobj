  this.WindowY = 480;
}
if(posWindowNum == 5)
{
  this.WindowX = 990;
  this.WindowY = 480;
}

OpenedTab = "Button_Tab_Settings"; //Открытая вкладка
Open_Settings.Invoke();

]]>
